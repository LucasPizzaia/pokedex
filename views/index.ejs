<!DOCTYPE html>
<html lang="pt-BR">
  <head>
    <meta charset="UTF-8" />
    <title>Pokédex</title>
    <link rel="stylesheet" href="home.css">
  </head>
  <body>
    <h1>Pokédex</h1>
    <ul>
      <% pokemons.forEach(function(pokemon) { %>
      <li>
        <a href="/pokemon/<%= pokemon.id %>"><%= pokemon.nome %> </a> - <%=
        pokemon.tipo %>
      </li>
      <% }); %>
    </ul>
  </body>
</html>

